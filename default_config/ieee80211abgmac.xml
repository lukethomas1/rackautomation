<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mac SYSTEM "file:///usr/share/emane/dtd/mac.dtd">
<mac library="ieee80211abgmaclayer">
  <param name="mode"                  value="3"/>  <!-- 0|2 = DSS, 1 = OFDM, 3 = Both -->
  <param name="enablepromiscuousmode" value="off"/>
  <param name="distance"              value="1000"/>
  <param name="unicastrate"           value="8"/> <!-- 1: 10kbps, 2: 25kbps, 3: 50kbps, 8: 1mbps -->
  <param name="multicastrate"         value="8"/>
  <param name="pcrcurveuri"
    value="/home/emane-01/emane_src/src/models/mac/ieee80211abg/ieee80211pcr.xml"/>
  <param name="flowcontrolenable"     value="off"/>
  <param name="flowcontroltokens"     value="10"/>
  <param name="radiometricenable"     value="off"/>
  <param name="retrylimit0"           value="4"/>
  <param name="retrylimit1"           value="4"/>
  <param name="retrylimit2"           value="4"/>
  <param name="retrylimit3"           value="4"/>
</mac>
